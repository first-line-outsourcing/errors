{
  "name": "@floteam/errors",
  "version": "1.0.8",
  "description": "Classes for working with errors",
  "private": false,
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "test:watch": "jest --watchAll --colors --passWithNoTests",
    "test": "jest --colors --passWithNoTests",
    "test:ci": "jest --no-colors --passWithNoTests",
    "prepare-husky": "husky install",
    "build-and-pack": "npm run build && shx cp package.json dist/ && shx cp README.md dist/ && shx cp mjs-wrapper/index.mjs dist/ && cd dist && npm pack",
    "postpack": "package-check"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "type": "commonjs",
  "module": "./index.mjs",
  "files": [
    "**/*"
  ],
  "keywords": [
    "errors"
  ],
  "author": "First Line Outsourcing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/first-line-outsourcing/errors.git"
  },
  "sideEffects": false,
  "dependencies": {
    "@redtea/format-axios-error": "^2.0.2",
    "@types/node": "^14.17.1",
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@skypack/package-check": "^0.2.2",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "27.0.3",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.0",
    "pretty-quick": "^3.1.0",
    "shx": "0.3.3",
    "ts-jest": "27.0.2",
    "ts-node": "^8.10.2",
    "typescript": "4.3.2"
  },
  "lint-staged": {
    "*": "prettier --write . --ignore-unknown"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./*": "./*.js"
  }
}
